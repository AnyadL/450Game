{
    "dialogue": 
    [
        {
            "Speaker": "Agni",
            "Text": "OMG, can you believe Dr. G cancelled class on us again yesterday?",
            "voFile": "",
            "displayTime": 4,
            "type": "Question",
            "special":  1
        },
        {
            "Speaker": "Agni",
            "Text": "I was stranded at school for hours!",
            "voFile": "",
            "displayTime": 3,
            "type":  "Exclamation"
        },
        {
            "Speaker": "Ryker",
            "Text": "Why didn’t you just run home, Agni?",
            "voFile": "",
            "displayTime": 2,
            "type":  "Question"
        },
        {
            "Speaker": "Agni",
            "Text": "Not everyone’s a speedster like you, Ryker!",
            "voFile": "",
            "displayTime": 3,
            "type":  "Exclamation"
        },
        {
            "Speaker": "Agni",
            "Text": "He better not have cancelled class again or I’m gonna flip!",
            "voFile": "",
            "displayTime": 4,
            "type":  "Exclamation"
        },
        {
            "Speaker": "Delilah",
            "Text": "I’m sure he didn’t.",
            "voFile": "",
            "displayTime": 2,
            "type":  "Ellipsis"
        },
        {
            "Speaker": "Kitty",
            "Text": "Didja miss me?",
            "voFile": "",
            "displayTime": 2,
            "type": "Question",
            "special": 2
        },
        {
            "Speaker": "Ryker",
            "Text": "Holy crap!  You just about gave me a heart attack, Kitty!",
            "voFile": "",
            "displayTime": 3,
            "type": "Exclamation",
            "special": 3
        },
        {
            "Speaker": "Ryker",
            "Text": "Hey, so… does something seem odd to you guys today?",
            "voFile": "",
            "displayTime": 4,
            "type":  "Question",
            "special": 4
        },
        {
            "Speaker": "Agni",
            "Text": "Hey, isn't that - ",
            "voFile": "",
            "displayTime": 2,
            "type":  "Ellipsis",
            "special": 5
        },
        {
            "Speaker": "Kitty",
            "Text": "Dr. G!",
            "voFile": "",
            "displayTime": 1,
            "type":  "Exclamation"
        },
        {
            "Speaker": "Dr. G",
            "Text": "Help!",
            "voFile": "",
            "displayTime": 1,
            "type":  "Exclamation"
        },
        {
            "Speaker": "Agni",
            "Text": "Oh, hell no!  There’s no way you’re cancelling class again!",
            "voFile": "",
            "displayTime": 3,
            "type":  "Exclamation"
        },
        {
            "Speaker": "Dr. G",
            "Text": "I’m being kidnapped!",
            "voFile": "",
            "displayTime": 3,
            "type":  "Exclamation"
        },
        {
            "Speaker": "Kitty",
            "Text": "We have to go after him!",
            "voFile": "",
            "displayTime": 2,
            "type": "Exclamation",
            "special":  6
        },
        {
            "Speaker": "Ryker",
            "Text": "Shouldn’t we let the real heroes deal with this?",
            "voFile": "",
            "displayTime": 3,
            "type":  "Question"
        },
        {
            "Speaker": "Delilah",
            "Text": "We’re the only ones here.",
            "voFile": "",
            "displayTime": 2,
            "type": "Ellipsis",
            "special": 7
        }
    ] 
}